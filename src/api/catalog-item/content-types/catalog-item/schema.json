{
  "kind": "collectionType",
  "collectionName": "catalog_items",
  "info": {
    "singularName": "catalog-item",
    "pluralName": "catalog-items",
    "displayName": "Catalog Items"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "richtext",
      "maxLength": 300
    },
    "PriceRUB": {
      "type": "decimal"
    },
    "Properties": {
      "type": "enumeration",
      "enum": [
        "brand",
        "workTime",
        "color",
        "article",
        "model"
      ]
    },
    "catalog_sections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::catalog-section.catalog-section",
      "mappedBy": "catalog_items"
    }
  }
}
